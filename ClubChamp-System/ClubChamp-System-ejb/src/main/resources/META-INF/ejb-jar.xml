<?xml version="1.0" encoding="UTF-8" ?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
 	<enterprise-beans>
         <session>
         	<ejb-name>ClubChampServiceBean</ejb-name>
         	<ejb-class>gruppe10.clubchamp.ClubChampServiceBean</ejb-class>         	
         	<env-entry>
	         	<env-entry-name>gruppe10.clubchamp.ClubChampServiceBean/clubname</env-entry-name>
	         	<env-entry-type>java.lang.String</env-entry-type>
	         	<env-entry-value>Club1</env-entry-value>
         	</env-entry>
         </session>
         <session>
         	<ejb-name>UserRegistry</ejb-name>
         	<ejb-class>gruppe10.user.UserRegistry</ejb-class>         	
         	<env-entry>
	         	<env-entry-name>gruppe10.user.UserRegistry/user1</env-entry-name>
	         	<env-entry-type>java.lang.String</env-entry-type>
	         	<env-entry-value>michael</env-entry-value>
         	</env-entry>
         	<env-entry>
	         	<env-entry-name>gruppe10.user.UserRegistry/password1</env-entry-name>
	         	<env-entry-type>java.lang.String</env-entry-type>
	         	<env-entry-value>123</env-entry-value>
         	</env-entry>
         </session>
	</enterprise-beans>
</ejb-jar>